<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>
	
	
	<servlet>
		<servlet-name>JavaStockServlet</servlet-name>
		<servlet-class>com.lisogurski.JavaStockServlet</servlet-class>
	</servlet>	
	<servlet-mapping>
		<servlet-name>JavaStockServlet</servlet-name>
		<url-pattern>/javastockservlet</url-pattern>
	</servlet-mapping>
		
	<servlet>
		<servlet-name>MyNewServlet</servlet-name>
		<servlet-class>com.lisogurski.MyNewServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>MyNewServlet</servlet-name>
		<url-pattern>/MyNewServlet</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>StockDetailsServlet</servlet-name>
		<servlet-class>com.lisogurski.StockDetailsServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>StockDetailsServlet</servlet-name>
		<url-pattern>/stockDetailsServlet</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>PortfolioServlet</servlet-name>
		<servlet-class>com.lisogurski.stock.servlet.PortfolioServlet</servlet-class>
	</servlet>	
	<servlet-mapping>
		<servlet-name>PortfolioServlet</servlet-name>
		<url-pattern>/portfolioServlet</url-pattern>
	</servlet-mapping>
	

	<servlet>
		<servlet-name>copyServlet</servlet-name>
		<servlet-class>com.lisogurski.model.CopyServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>copyServlet</servlet-name>
		<url-pattern>copyServlet</url-pattern>
	</servlet-mapping>
	
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>
</web-app>
